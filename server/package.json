    "seed:admin": "ts-node-dev src/modules/users/infra/typeorm/seeds/admin.ts",
    "seed:test": "ts-node-dev src/modules/users/infra/typeorm/seeds/test.ts",
    "seed:seats": "ts-node-dev src/modules/seats/infra/typeorm/seeds/seat.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files"
  },
  "dependencies": {
    "argon2": "^0.28.5",
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
