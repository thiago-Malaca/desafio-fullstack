    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.8",
    "supertest": "^6.2.3",
    "swagger-ui-express": "^4.3.0",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.2.35",
    "uuid": "^8.3.2"
